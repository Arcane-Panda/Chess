#include <iostream>
using namespace std;



int main()
{
  char board[8][8] = { {'p','p',' ',' ',' ',' ','p','p'}, {'p','p',' ',' ',' ',' ','p','p'},
  {'p','p',' ',' ',' ',' ','p','p'}, {'K','p',' ',' ',' ',' ','p','Q'},
  {'Q','p',' ',' ',' ',' ','p','K'}, {'p','p',' ',' ',' ',' ','p','p'},
  {'p','p',' ',' ',' ',' ','p','p'}, {'p','p',' ',' ',' ',' ','p','p'},};

  for (int j = 0; j < 8; j++)
  {
    for (int i = 0; i < 8; i++)
    {
      cout << board[i][j];
    }
    cout << endl;
  }

  return 0;
